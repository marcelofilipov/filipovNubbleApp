{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "noImplicitAny": true,
    "target": "ES6",
    "strictNullChecks": true,
    "baseUrl": "./",
    "paths": {
      "@components": [
        "src/components"
      ],
      "@hooks": [
        "src/hooks"
      ],
      "@routes": [
        "src/routes"
      ],
      "@screens": [
        "src/screens"
      ],
      "@theme": [
        "src/theme"
      ],
      "@domain": [
        "src/domain"
      ]
    }
  },
}